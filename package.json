{
  "name": "@gisatcz/ptr-state",
  "version": "1.5.13",
  "description": "Panther FE state",
  "keywords": [
    "panther",
    "gisat"
  ],
  "homepage": "https://github.com/gisat-panther/ptr-state",
  "style": "dist/style.css",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "lib"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gisat-panther/ptr-state"
  },
  "dependencies": {
    "@gisatcz/ptr-core": "^1.3.10",
    "@gisatcz/ptr-utils": "^1.3.12",
    "@manaflair/redux-batch": "1.0.0",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.20",
    "minimist": "1.2.5",
    "moment": "^2.29.1",
    "path": "0.12.7",
    "query-string": "^6.14.0",
    "re-reselect": "^4.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "6.0.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.12.13",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "auto": "^10.16.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.3.0",
    "cross-env": "^7.0.2",
    "glob": "^7.1.6",
    "mocha": "^8.3.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.7.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^9.1.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "slash": "3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "run-s clean && run-p start:es start:cjs start:lib:es",
    "start:lib:es": "cross-env BABEL_ENV=es babel --watch src -d lib --verbose",
    "start:cjs": "cross-env NODE_ENV=cjs rollup -c -w",
    "start:es": "cross-env NODE_ENV=es rollup -c -w",
    "build": "run-s clean && run-p build:es build:cjs build:lib:es",
    "build:es": "cross-env NODE_ENV=es rollup -c",
    "build:cjs": "cross-env NODE_ENV=cjs rollup -c",
    "build:lib:es": "npm-run-all build:babel:lib:es",
    "build:babel:lib:es": "cross-env BABEL_ENV=es babel src -d lib",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=html --reporter=json npm run test",
    "format": "prettier --write ."
  }
}
